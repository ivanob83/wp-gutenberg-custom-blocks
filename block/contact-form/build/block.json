{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "cc/contact-form",
  "title": "Contact Form",
  "category": "cc-blocks",
  "icon": "email",
  "description": "Contact form section with Contact Form 7",
  "keywords": ["contact", "form", "cf7"],
  "version": "1.0.0",
  "textdomain": "CC",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./index.css",
  "attributes": {
    "title": {
      "type": "string",
      "default": "Contact Us"
    },
    "subtitle": {
      "type": "string",
      "default": "Your email address will not be published. Required fields are marked *"
    },
    "formShortcode": {
      "type": "string",
      "default": "[contact-form-7 title=\"Main Contact Form\"]"
    }
  }
}
